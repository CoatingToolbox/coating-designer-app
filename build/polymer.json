{
  "entrypoint": "index.html",
  "shell": "src/app-shell.js",
  "fragments": [
    "src/pages/home-page.js",
    "src/pages/tablet-pages.js",
    "src/tablet/tablet-overview-page.js",
    "src/tablet/tablet-designer-page.js",
    "src/tablet/tablet-library-page.js",
    "src/pages/pan-pages.js",
    "src/pan/pan-overview-page.js",
    "src/pan/pan-designer-page.js",
    "src/pan/pan-library-page.js",
    "src/pages/coating-pages.js",
    "src/coating/coating-overview-page.js",
    "src/coating/coating-designer-page.js",
    "src/coating/coating-library-page.js"
  ],
  "sources": [
    "src/**/*",
    "manifest/**",
    "package.json",
    "index.html",
    "src/app-shell.js",
    "src/pages/home-page.js",
    "src/pages/tablet-pages.js",
    "src/tablet/tablet-overview-page.js",
    "src/tablet/tablet-designer-page.js",
    "src/tablet/tablet-library-page.js",
    "src/pages/pan-pages.js",
    "src/pan/pan-overview-page.js",
    "src/pan/pan-designer-page.js",
    "src/pan/pan-library-page.js",
    "src/pages/coating-pages.js",
    "src/coating/coating-overview-page.js",
    "src/coating/coating-designer-page.js",
    "src/coating/coating-library-page.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest/manifest.json",
    "node_modules/redux/dist/redux.min.js",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/firebase/firebase.js",
    "node_modules/d3/dist/d3.js"
  ],
  "builds": [
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": {
          "exclude": [
            "node_modules/redux/dist/redux.min.js",
            "node_modules/firebase/firebase.js",
            "node_modules/d3/dist/d3.js"
          ]
        },
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    }
  ],
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}