{
    "entrypoint": "index.html",
    "shell": "src/cd-app-shell.js",
    "fragments": [
        "src/pages/cd-home-page.js",
        "src/pages/cd-tablet-pages.js",
        "src/pages/cd-tablet-overview-page.js",
        "src/pages/cd-tablet-design-page.js"
    ],
    "sources": [
        "src/**/*",
        "manifest/**",
        "package.json"
    ],
    "extraDependencies": [
        "manifest/manifest.json",
        "node_modules/redux/dist/redux.min.js",
        "node_modules/@webcomponents/webcomponentsjs/**",
        "node_modules/firebase/firebase.js"
    ],
    "builds": [{
        "preset": "es6-unbundled",
        "addServiceWorker": true,
        "js": {
            "minify": {
                "exclude": [
                    "node_modules/redux/dist/redux.min.js", 
                    "node_modules/firebase/firebase.js"
                ]
            }
        }
    }],
    "moduleResolution": "node",
    "npm": true
}
